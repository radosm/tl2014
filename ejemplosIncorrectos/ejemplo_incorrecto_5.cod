/*En este ejemplo evaluamos como punto de falla el pasar distinta cantidad de parametros
de los que estan definidos. 
*/
function id(x) {
    return x
}

function fact(x) {
    if x==0 || x==1 then
        return 1
    i = 1
    while x > 0 {
        i = i*x
        x = x-1
    } 
    return i
}


plot (id(x,x), fact(x)) for x = 1..1..6
